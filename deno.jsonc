{
  "name": "@mityu/lspoints-toolkit",
  "version": "0.1.2",
  "exports": {
    "./textprop": "./textprop.ts",
    "./popup": "./popup.ts",
    "./highlight": "./highlight.ts",
    "./markup-text": "./markup_text/markup_text.ts",
    "./to-vim-positions": "./to_vim_positions.ts",
    "./to-vim-ranges": "./to_vim_ranges.ts",
    "./echo": "./echo.ts"
  },
  "lock": false,
  "tasks": {
    "check-all": "deno task check && deno task lint && deno task test",
    "check": "deno check --no-lock ./",
    "lint": "deno lint ./",
    "lint-fix": "deno lint --fix ./",
    "fmt": "deno fmt ./",
    "test": "deno test -A --doc --parallel --shuffle **/*.ts",
    "upgrade": "deno run -A jsr:@molt/cli **/*.ts --write"
  }
}
